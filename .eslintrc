{
  "extends": "airbnb/base",

  "globals": {
    "_gaq": true,
    "_pa": true,
    "$": true,
    "app": true,
    "alert": true,
    "AudioController": true,        
    "canvg": true,
    "console": true,
    "escape": true,
    "WAGNER": true,
    "module": true,
    "require": true,
    "System": true,
    "user": true,
    "TweenMax": true,
    "TimelineMax": true,
    "Power1": true,
    "Power2": true,
    "Power3": true,
    "Power4": true,
    "Stage3d": true,
    "Signals": true,
    "window": true,
  },
  "rules": {
    "class-methods-use-this": 0,
    "import/no-extraneous-dependencies": 0,
    "func-names": 0,
    "no-case-declarations": 0,
    "no-mixed-operators": 0,
    "no-multi-assign": 0,
    "no-param-reassign": 0,
    "no-plusplus": 0,
    "no-underscore-dangle": 0,
    "no-unresolved": 0,
    "no-unused-vars": 1,
    "no-useless-escape": 0,
    "no-new": 0,
    "no-new-func": 0,
    "no-console": 0,
    "padded-blocks": 0,
    "id-length": 0,
    "space-in-parens": 0,
    "max-len": [1, 150, 4],
    "semi": 0
  },
}
